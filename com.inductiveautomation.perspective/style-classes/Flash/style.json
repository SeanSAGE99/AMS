{
  "base": {
    "animation": {
      "duration": "1s",
      "timingFunction": "ease",
      "keyframes": {
        "0%": {
          "backgroundColor": "#FFFFFF",
          "opacity": "1",
          "stroke": "#FFFFFF",
          "fill": "#FFFFFF"
        },
        "100%": {
          "backgroundColor": "#AAAAAA",
          "opacity": "1",
          "stroke": "#AAAAAA",
          "fill": "#AAAAAA"
        }
      }
    }
  }
}