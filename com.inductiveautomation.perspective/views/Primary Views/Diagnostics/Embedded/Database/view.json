                "binding": {
                  "config": {
                    "fallbackDelay": 2.5,
                    "mode": "direct",
                    "tagPath": "[System]Gateway/Performance/CPU Usage"
                  },
                  "transforms": [
                    {
                      "expression": "{value} * 100.0",
                      "type": "expression"
                    },
                    {
                      "fallback": "#808080",
                      "inputType": "range",
                      "mappings": [
                        {
                          "input": {
                            "max": 70,
                            "min": 0
                          },
                          "output": "#00AC00"
                        },
                        {
                          "input": {
                            "max": 80,
                            "min": 70
                          },
                          "output": "#D9D900"
                        },
                        {
                          "input": {
                            "max": 100,
                            "min": 90
                          },
