  "params": {
    "params": {
      "areaPath": "[default]International/T1/Pier C/Arrivals"
    },
    "path": "Primary Views/Diagnostics/Embedded/GPU PCA Devices",
    "title": "Arrivals",
    "useDefaultHeight": false,
    "useDefaultWidth": false
  },
  "propConfig": {
    "custom.instances": {
      "binding": {
        "config": {
          "expression": "now(60000)"
        },
        "transforms": [
          {
            "code": "\t#check the instances of \"GPU and PCA\" UDT types in the areaPath\n\tinstances \u003d []\n\tres \u003d system.tag.browse(self.params.params.areaPath, {\"tagType\":\"UdtInstance\", \"recursive\":True, \u0027name\u0027: \u0027*Ground Power Unit*\u0027 })#\u0027*Ground Power Unit*\u0027or \u0027*Preconditioning Air Unit*\u0027\n\tres2 \u003d system.tag.browse(self.params.params.areaPath, {\"tagType\":\"UdtInstance\", \"recursive\":True, \u0027name\u0027: \u0027*Preconditioning Air Unit*\u0027 })\n\tfor result in res.getResults():\n\t    deviceTagPath \u003d result[\"fullPath\"]\n\t    \t    \n\t    # Check if the GPU instance is enabled. it is done by checking if the \"running\" tag is enabled for the instance\n\t    modeTagEnabledPath \u003d \"{}/Status/Running.Enabled\".format(deviceTagPath)\n\t    modeTagEnabled \u003d system.tag.read(modeTagEnabledPath)\n\t            \n\t    if modeTagEnabled.value:\n\t\t    instances.append({\"deviceTagPath\":result[\"fullPath\"],\"deviceName\":result[\"name\"],\"type\":\"GP\"})\n\tfor result in res2.getResults():\n\t\t    deviceTagPath \u003d result[\"fullPath\"]\n\t\t    \t    \n\t\t    # Check if the GPU instance is enabled. it is done by checking if the \"running\" tag is enabled for the instance\n\t\t    modeTagEnabledPath \u003d \"{}/Status/Running.Enabled\".format(deviceTagPath)\n\t\t    modeTagEnabled \u003d system.tag.read(modeTagEnabledPath)\n\t\t            \n\t\t    if modeTagEnabled.value:\n\t\t\t    instances.append({\"deviceTagPath\":result[\"fullPath\"],\"deviceName\":result[\"name\"],\"type\":\"PCA\"})\n\treturn instances",
            "type": "script"
          }
        ],
        "type": "expr"
      },
      "persistent": true
    },
    "params.params": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.path": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.title": {
      "paramDirection": "input",
      "persistent": true
    },
