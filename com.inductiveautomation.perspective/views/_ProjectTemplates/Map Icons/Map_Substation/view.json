{
  "custom": {},
  "params": {
    "deviceTagPath": "value",
    "navigationLabel": "value",
    "navigationPage": "value",
    "visible": true,
    "zoomLevel": 13
  },
  "propConfig": {
    "params.deviceTagPath": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.navigationLabel": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.navigationPage": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.visible": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.zoomLevel": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 30,
      "width": 60
    }
  },
  "root": {
    "children": [
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "page": "{view.params.navigationPage}"
              },
              "scope": "C",
              "type": "nav"
            }
          }
        },
        "meta": {
          "name": "Icon",
          "tooltip": {
            "enabled": true
          }
        },
        "position": {
          "height": 30,
          "width": 30
        },
        "propConfig": {
          "meta.tooltip.text": {
            "binding": {
              "config": {
                "path": "view.params.navigationLabel"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "color": "#2B2B2B",
          "path": "material/offline_bolt",
          "style": {
            "cursor": "pointer"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "meta": {
          "name": "Symbol_Double",
          "tooltip": {
            "enabled": true
          }
        },
        "position": {
          "height": 30,
          "width": 30,
          "x": 30
        },
        "propConfig": {
          "meta.visible": {
            "binding": {
              "config": {
                "expression": "tag ({view.params.deviceTagPath}+\u0027/Alarm Summary/Active Unack\u0027)"
              },
              "transforms": [
                {
                  "code": "\tif \tvalue \u003e 0:\n\t\tvalue \u003d 1\n\telse:\n\t\tvalue \u003d 0\n\t\t\n\treturn value",
                  "type": "script"
                }
              ],
              "type": "expr"
            }
          },
          "props.params.alarmSummary.parentPath": {
            "binding": {
              "config": {
                "expression": "{view.params.deviceTagPath}"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "params": {
            "alarmSummary": {
              "enabled": true
            },
            "device": {
              "enabled": false,
              "name": null,
              "parentPath": null
            }
          },
          "path": "Shared/Alarms/Symbol"
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root",
      "tooltip": {
        "text": "value"
      }
    },
    "propConfig": {
      "meta.visible": {
        "binding": {
          "config": {
            "path": "view.params.visible"
          },
          "type": "property"
        }
      }
    },
    "props": {
      "style": {
        "backgroundColor": "transparent",
        "overflow": "hidden"
      }
    },
    "type": "ia.container.coord"
  }
}